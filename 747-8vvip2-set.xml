						setprop("/controls/flight/rudder", 0);
						setprop("/controls/flight/aileron-trim", 0);
						setprop("/controls/flight/rudder-trim", 0);
					</script>
  				</binding>
 			</key>

			<key n="66">
				<name>B</name>
				<desc>Toggle parking brake on or off</desc>
				<binding>
					<command>nasal</command>
					<script>
